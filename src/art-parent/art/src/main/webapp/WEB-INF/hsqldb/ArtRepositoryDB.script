INSERT INTO ART_QUERIES VALUES(0,83,'C3.js 1',1,NULL,'sample-c3js1.js','','','Shows a pie chart with sold items in the selected ','null',NULL,142,'',0,'2017-02-20 11:57:16.617000','admin',NULL,0,0,NULL,1,0,'default','','','','','','','','','',0,'',NULL,0,0,NULL,NULL,NULL,'/*\u000d\u000a * Purpose: \u000d\u000a * show pie chart\u000d\u000a * show how a LoV can be used\u000d\u000a * show how inline date parameters can be used \u000d\u000a *\u000d\u000a */\u000d\u000aSELECT i.name "Item", \u000d\u000a       SUM(o.VOLUME) "Volume"\u000d\u000a  FROM ORDERS o, ITEMS i, REGIONS r, CITIES c\u000d\u000a WHERE r.REGION_ID = c.REGION_ID\u000d\u000a   AND o.CITY_ID  = c.CITY_ID\u000d\u000a   AND o.ITEM_ID  = i.ITEM_ID\u000d\u000a   AND r.REGION_ID = #regionId# --the lov passes the first column\u000d\u000a   AND o.ORDER_DATE BETWEEN #startDate# AND #endDate#\u000d\u000a GROUP BY i.NAME\u000d\u000a ORDER BY 1',NULL,'Items Volume','Shows a pie chart with sold items in the selected region and specified period')
INSERT INTO ART_QUERIES VALUES(0,84,'C3.js 2',1,'2017-02-20 20:20:13.359000','sample-c3js2.js','','','Shows a pie chart with sold items in the selected ','null',NULL,142,'',0,'2017-02-20 12:02:44.535000','admin','admin',0,0,NULL,1,0,'default','','','','','','','','','',0,'{\u000d\u000a    "cssFile": "sample-c3js2.css"\u000d\u000a}',NULL,0,0,NULL,NULL,NULL,'/*\u000d\u000a * Purpose: \u000d\u000a * show pie chart\u000d\u000a * show how a LoV can be used\u000d\u000a * show how inline date parameters can be used \u000d\u000a *\u000d\u000a */\u000d\u000aSELECT i.name "Item", \u000d\u000a       SUM(o.VOLUME) "Volume"\u000d\u000a  FROM ORDERS o, ITEMS i, REGIONS r, CITIES c\u000d\u000a WHERE r.REGION_ID = c.REGION_ID\u000d\u000a   AND o.CITY_ID  = c.CITY_ID\u000d\u000a   AND o.ITEM_ID  = i.ITEM_ID\u000d\u000a   AND r.REGION_ID = #regionId# --the lov passes the first column\u000d\u000a   AND o.ORDER_DATE BETWEEN #startDate# AND #endDate#\u000d\u000a GROUP BY i.NAME\u000d\u000a ORDER BY 1',NULL,'Items Volume','C3.js - pie with custom label - label includes data value')
INSERT INTO ART_QUERIES VALUES(0,85,'C3.js 3',1,NULL,'sample-c3js3.js','','','X axis Label','Y axis Label',NULL,142,'',0,'2017-02-20 12:19:20.726000','admin',NULL,0,0,NULL,1,0,'default','','','','','','','','','',0,'',NULL,0,0,NULL,NULL,NULL,'/*\u000d\u000a * Purpose: \u000d\u000a * show bar char, how to sort bars in a bar chart\u000d\u000a *  how to change default size (see Short Description)\u000d\u000a * show how inline multi-params can be used \u000d\u000a *\u000d\u000a */\u000d\u000a\u000d\u000aSELECT TO_CHAR ( o.ORDER_DATE, ''MONTH'' ) "Month",  \u000d\u000a       r.NAME "Name" ,\u000d\u000a       SUM(o.VOLUME) "Value"  \u000d\u000a  FROM ORDERS o, ITEMS i, REGIONS r, CITIES c\u000d\u000a WHERE r.REGION_ID = c.REGION_ID\u000d\u000a   AND o.CITY_ID  = c.CITY_ID\u000d\u000a   AND o.ITEM_ID  = i.ITEM_ID\u000d\u000a   AND TO_CHAR (o.ORDER_DATE,''YYYY'') = #year#\u000d\u000a   AND r.REGION_ID in (#regionIds#)\u000d\u000a GROUP BY TO_CHAR ( o.ORDER_DATE, ''MONTH'' ) ,r.NAME\u000d\u000a-- ORDER BY TO_CHAR ( o.ORDER_DATE, ''YYYY-MM'' )\u000d\u000a -- The last order by is to sort correctly the month that would\u000d\u000a -- be otherwise sorted alphabetically (i.e. Feb before Jan etc)',NULL,'Yearly  Volume - Chart','C3.js - vertical bar chart - dynamic series')
